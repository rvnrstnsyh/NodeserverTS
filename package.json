{
    "name": "nodeserverts",
    "version": "1.0.0",
    "description": "Back-End node server",
    "main": "index.ts",
    "type": "commonjs",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/rvnrstnsyh/nodeserverts.git"
    },
    "bugs": {
        "url": "https://gitlab.com/rvnrstnsyh/nodeserverts/-/issues",
        "email": "re@rvnrstnsyh.dev"
    },
    "scripts": {
        "prod": "nodemon ./dist/src/index.js",
        "dev": "tsc-watch --onSuccess \"nodemon ./dist/src/index.js\"",
        "build": "tsc",
        "postinstall": "npm run build",
        "ignite": "bash ignite.sh",
        "ext": "rm -rf node_modules dist package-lock.json yarn.lock access.log .env"
    },
    "keywords": [
        "Node.js",
        "TypeScript",
        "server",
        "backend",
        "API"
    ],
    "author": "Rivane Rasetiansyah <re@rvnrstnsyh.dev> (https://rvnrstnsyh.dev)",
    "contributors": [
        "Rivane Rasetiansyah <re@rvnrstnsyh.dev> (https://rvnrstnsyh.dev)"
    ],
    "license": "GPL-3.0",
    "engines": {
        "node": ">= 18.12.0",
        "npm": ">= 8.5.5",
        "yarn": ">= 1.22.17"
    },
    "devDependencies": {
        "@types/compression": "^1.7.2",
        "@types/connect-flash": "^0.0.37",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.12",
        "@types/crypto-js": "^4.1.1",
        "@types/csurf": "^1.11.2",
        "@types/express": "^4.17.14",
        "@types/express-session": "^1.17.5",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/morgan": "^1.9.3",
        "@types/node": "^18.11.10",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "nodemon": "^2.0.20",
        "prettier": "^2.8.0",
        "tsc-watch": "^5.0.3",
        "typescript": "^4.9.3"
    },
    "dependencies": {
        "argon2": "^0.30.2",
        "compression": "^1.7.4",
        "connect-flash": "^0.1.1",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "crypto-js": "^4.1.1",
        "csrf": "^3.1.0",
        "csurf": "^1.11.0",
        "dotenv": "^16.0.3",
        "envalid": "^7.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^6.7.0",
        "express-session": "^1.17.3",
        "helmet": "^6.0.1",
        "joi": "^17.7.0",
        "jsonwebtoken": "^8.5.1",
        "module-alias": "^2.2.2",
        "mongoose": "^6.7.5",
        "morgan": "^1.10.0"
    },
    "_moduleAliases": {
        "@/middleware": "./dist/src/middleware",
        "@/resources": "./dist/src/resources",
        "@/utils": "./dist/src/utils"
    },
    "nodemonConfig": {
        "ignore": [
            "./private/**/*",
            "./dist/private/**/*"
        ],
        "delay": 1000
    }
}
